define ['traits'], (trait)->
   
   define: trait
   
   create: trait.create
   
   compose: trait.compose
   
   resolve: trait.resolve
   
   override: trait.override
   
   required: trait.required
   
   is_equal: trait.eqv
   
   object: trait.object